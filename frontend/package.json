{
  "name": "ottawa-genai-prototype",
  "version": "1.0.0",
  "description": "High-fidelity prototype for Ottawa Economic Development GenAI Research Assistant",
  "author": "City Studio Program - Algonquin College",
  "license": "MIT",
  "keywords": [
    "ottawa",
    "economic-development",
    "genai",
    "research-assistant",
    "react",
    "typescript",
    "accessibility",
    "wcag",
    "bilingual"
  ],
  "homepage": ".",
  "private": true,
  "dependencies": {
    "@react-oauth/google": "0.12.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.8.0",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "preinstall": "node --version && npm --version",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:integration": "node tests/integration/run-integration-tests.js",
    "eject": "react-scripts eject",
    "build:prod": "npm run build",
    "serve": "npx serve -s build -l 3000",
    "analyze": "npx source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
    "type-check": "tsc --noEmit",
    "check-deployment": "node check-deployment.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@playwright/test": "^1.40.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "msw": "^2.0.0",
    "node-fetch": "3.3.2"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhizhunbao/ottawa-genai-research-assistant"
  },
  "bugs": {
    "url": "https://github.com/zhizhunbao/ottawa-genai-research-assistant/issues"
  }
}
